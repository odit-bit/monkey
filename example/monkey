
let adderTwo = fn (x) {x + 2;}

let minAddertwo = fn(x, adderTwo) {return x - adderTwo(x);}

//print to stdout
puts(minAddertwo(10, adderTwo))

// concat string
let a = "hello";
let b = "world";

let concatWord = fn (a,b) {return a + " " + b;};


puts(concatWord(a,b)) // print concatWord


puts(len(concatWord(a,b))) //print length of "hello world"


let arr = ["hello", "world", 2000 + 24];
puts(arr)// [hello, world, 2024]